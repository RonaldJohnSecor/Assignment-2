# Assignment in Computer Programming

# Transpose Method

# EXAMPLE A
import numpy as np
A = np.array([[1, 4], [3, 5]])

result = A.transpose()

print(f"EXAMPLE A: {result}")

# EXAMPLE B
import numpy as np

A = np.array([[3, 4, 6], [7, 10, 9], [6, 5, 3]])

result = A.transpose()

print(f"EXAMPLE B: {result}")

# EXAMPLE C
import numpy as np

A = np.array([[2,4,3], [8,5,4], [7, 4, 6]])

result = A.transpose()

print(f"EXAMPLE C: {result}")