# linalg.det method

# EXAMPLE A
import numpy as np
from numpy.linalg import det

# Define a matrix
A = np.array([[5, 4], [6, 7]])

result = det(A)

print(f'EXAMPLE A:{result}')

# EXAMPLE B
import numpy as np
from numpy.linalg import det

A = np.array([[1, 3], [6, 5]])

result = det(A)
print(f'EXAMPLE B:{result}')

# EXAMPLE C
import numpy as np
from numpy.linalg import det

A = np.array([[2, 4, 6], [8, 5, 6], [7, 5, 3]])

result = det(A)
print(f'EXAMPLE C:{result}')
